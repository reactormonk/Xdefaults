Xcursor.theme: ArchCursorBlue
!------------------!
!   URvxt Stuff    !
!------------------!

URxvt*scrollTtyOutput: false

!--[term name]--!
!URxvt*termName: rxvt-256color
URxvt*termName: rxvt-unicode

!--[trans and fg/bg]--!
URxvt*tintColor: lightblue
URxvt*transparent: true
URxvt*shading: 25
URxvt*depth: 24
URxvt*borderless: 1

!--[font]--! 
!URxvt*font: -*-terminus-*-*-*-*-16-*
!xft:Bitstream Vera Sans Mono:size=9
!URxvt*font: -*-dina-*-r-*-*-15-*-*-*-*-*-*-*
!URxvt*boldFont: -*-terminus-bold-*-*-*-12-*-*-*-*-*-*-u
URxvt.font: xft:DejaVu Sans Mono:autohint=true:pixelsize=12
!URxvt*boldFont:			xft:DejaVu Sans Mono:pixelsize=8:bold:autohint=true
!URxvt*italicFont:		xft:DejaVu Sans Mono:pixelsize=8:italic:autohint=true
!URxvt*boldItalicFont:		xft:DejaVu Sans Mono:pixelsize=8:bold:italic:autohint=true
!URxvt.font: xft:Bitstream Vera Sans Mono:pixelsize=32
Xft.antialias: 1
Xft.hinting:    1
Xft.hintstyle:  hintfull
Xft.rgba:       rgb


!--[scrolling]--! 
URxvt*scrollBar: false
URxvt*saveLines: 10000

!--[tabs]--! 
!URxvt*perl-ext-common: default,tabbed

!--[urls]--!
URxvt.perl-lib:        /home/tass/.urxvt
URxvt.perl-ext:        default,matcher,font-size
URxvt.urlLauncher:     /usr/bin/chromium
URxvt.matcher.button:  1 


!--[cursor]--!
URxvt*cursorBlink: true

!--[Colours]--! 
URxvt*cursorColor: #FFAF00
URxvt*cursorColor2: #18F018
URxvt*background: #000000
URxvt*foreground: #18F018

! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white

!URxvt*color0:    #6c6c6c
URxvt*color1:    #ff3300
!URxvt*color2:    #b6e77d
!URxvt*color3:    #ecebbe
URxvt*color4:    #0035a8
!URxvt*color5:    #ea96eb
!URxvt*color6:    #c9caec
!URxvt*color7:    #f2f2f2

! Light
! color8    black
! color9    red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    white

!URxvt*color8:    #747474
!URxvt*color9:    #f99286
!URxvt*color10:    #c3f786
!URxvt*color11:    #fcfbcc
URxvt*color12:    #8090ff
!URxvt*color13:    #fba1fb
!URxvt*color14:    #d7d9fc
!URxvt*color15:    #e2e2e2

URxvt.keysym.Mod4-plus: perl:font:increase
URxvt.keysym.Mod4-minus: perl:font:decrease
